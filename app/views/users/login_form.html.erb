<div class="main users-new">
  <div class="container">
    <div class="form users-from">
      <div class="form-body">
        <% if @error_message %>
        <div class="from-error">
          <span class="error"><%= @error_message %></span>
        </div>
        <% end %>
        <%= form_tag("/login") do %>
         <p>メールアドレス</p>
         <input name="email" value="<%= @email %>">
         <p>パスワード</p>
         <input type="password" name="password" value="<%= @password %>">
         <input type="submit" value="ログイン">
        <% end %>
      </div>
    </div>
    <P>test ID</P>
    <P>"一般ユーザー"</P>
    <P>ID xxx@docomo.ne.jp</P>
    <p>pass xxxxxx</p>

    <P>"トレーナーユーザー"</P>
    <P>ID yyy@docomo.ne.jp</P>
    <p>pass yyyyyy</p>
  </div>
</div>
